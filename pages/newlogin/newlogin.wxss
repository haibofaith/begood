.parent-area {
  justify-content: center;
  width: 100%;
  height: 100%;
  position: absolute;
}

.open-data-userAvatarUrl {
  width: 160rpx;
  height: 160rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
  margin: auto;
}

.open-data-userNickName {
  margin: 12px auto;
  display: block;
  text-align: center;
  font-size: 16px;
  color: black;
  font-weight: bolder;
}

.divLine {
  margin-top: 30px;
  background: #e0e3da;
  height: 3rpx;
  margin-left: 35px;
  margin-right: 35px;
}

#weixin-img {
  display: inline-block;
  margin-top: 80px;
  width: 100px;
  height: 100px;
  border-radius: 50%;
}

.weixin-parent {
  text-align: center;
  width: 100%;
}

#user-perms {
  margin-top: 140px;
  text-align: center;
  color: #fff;
  font-size: 16px;
  padding: 2px;
  margin-left: 45px;
  margin-right: 45px;
  background: #09bb07;
  font-family: Monospace;
}

#tv-1-parent {
  margin-top: 25px;
}

#tv-2-parent {
  margin-top: 5px;
}

#tv-1 {
  margin-left: 40px;
  text-align: left;
  font-size: 14px;
  color: #333;
  font-weight: bolder;
}

#tv-2 {
  margin-left: 40px;
  text-align: left;
  font-size: 12px;
  color: #666;
  font-weight: inherit;
}

@font-face {
  font-family: 'iconfont';  /* project id 1075360 */
  src: url('//at.alicdn.com/t/font_1075360_ious72beekp.eot');
  src: url('//at.alicdn.com/t/font_1075360_ious72beekp.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1075360_ious72beekp.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1075360_ious72beekp.woff') format('woff'),
  url('//at.alicdn.com/t/font_1075360_ious72beekp.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1075360_ious72beekp.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-yuandianzhong:before {
  content: "\e830";
  vertical-align:middle;
  margin-right: 5px;
}

.icon-yuandian:before {
  content: "\e61a";
}

.icon-yuandian1:before {
